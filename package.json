{
  "name": "ng2-seed",
  "version": "0.0.0",
  "description": "A simple starter Angular2 project",
  "scripts": {
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --progress --profile --colors --display-error-details --display-cached  --content-base src/ --env dev",
    "server:prod": "http-server dist --cors",
    "docs": "gulp docs",
    "build": "npm run build:dev",
    "build:dev": "webpack --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "webpack --config webpack.build.config.js --progress --profile --colors --display-error-details --display-cached",
    "test": "karma start",
    "test:live": "karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hirsch88/ng2-seed"
  },
  "keywords": [
    "Angular2",
    "ng2-seed"
  ],
  "author": "Gery Hirschfeld <gery.hirschfeld@w3tec.ch>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hirsch88/ng2-seed/issues"
  },
  "homepage": "https://github.com/hirsch88/ng2-seed#readme",
  "dependencies": {
    "angular2": "2.0.0-beta.0",
    "bootstrap-sass": "^3.3.6",
    "es6-promise": "3.2.1",
    "es6-shim": "0.35.1",
    "moment": "^2.11.2",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.0",
    "url-loader": "^0.5.7",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "bootstrap-sass": "^3.3.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.1.0",
    "chalk": "^1.1.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs-extra": "^0.30.0",
    "gulp": "^3.8.11",
    "gulp-connect": "^4.0.0",
    "gulp-karma": "0.0.5",
    "gulp-task-listing": "^1.0.1",
    "gulp-typedoc": "^2.0.0",
    "html-webpack-plugin": "^2.7.2",
    "json-loader": "^0.5.4",
    "karma": "^0.13.21",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.3",
    "karma-coverage": "^1.0.0",
    "karma-growl-reporter": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-typescript-preprocessor2": "^1.1.1",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.13.1",
    "lodash.merge": "^4.3.1",
    "node-sass": "^3.4.2",
    "phantomjs": "^2.1.7",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
